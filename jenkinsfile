node{
    stage('Checkout SCM'){
        git 'https://github.com/Vishwanathms/SampleMaven'
    }
    stage('Compile-Package'){
        def mvnHome = tool name: 'maven-3.6.1', type: 'maven'
        sh "${mvnHome}/bin/mvn package"
    }
    stage('Docker deploy'){
        sh 'docker build .' 
    }
}
